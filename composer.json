{
    "name": "dalsn/cashworx",
    "description": "A PHP package for using Cashworx's API",
    "keywords": [
        "cashworx",
        "package",
        "api",
        "wrapper"
    ],
    "homepage": "https://github.com/dalsn/cashworx",
    "license": "MIT",
    "authors": [
        {
            "name": "Dalhatu Njidda",
            "email": "dalhatu@longtailtech.com.ng",
            "homepage": "http://dalhatu.com",
            "role": "Software Developer"
        }
    ],
    "require": {
        "php" : ">=5.6.4",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Dalsn\\Cashworx\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dalsn\\Cashworx\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
